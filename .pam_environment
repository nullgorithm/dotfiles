EDITOR=vim
VISUAL=vim
PAGER=less
TERMINAL=termite
BROWSER=firefox

# use a 32-bit wine prefix
WINEARCH=win32

# disable wine debug messages
WINEDEBUG=-all

# disable wine desktop integration (menu items, desktop links, mimetypes)
WINEDLLOVERRIDES=winemenubuilder.exe=d

GREP_OPTIONS=--color=auto
LESS=-R

SDL_AUDIODRIVER=alsa
